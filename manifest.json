{
  // Required
  "name": "Soccer Live Update",
  "version": "1.0",

  // Recommended
  "description": "Chrome Extension to be Updated with Soccer Event",
  "icons": {
			"16": "img/icon16.png",
			"48": "img/icon48.png",
			"128": "img/icon128.png"
			},

  // Pick one (or none)
  "browser_action": {
					"default_icon": "img/icon16.png",
					"popup": "popup.html"
					},

  // Add any of these that you need
  "permissions": ["http://*/*"]
}

